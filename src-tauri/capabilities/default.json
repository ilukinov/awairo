{
  "$schema": "../gen/schemas/capabilities.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "global-shortcut:default",
    "store:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.track.toggl.com/*"
        }
      ]
    }
  ]
}